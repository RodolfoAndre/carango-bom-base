{
  "name": "carango-bom",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/data-grid": "^4.0.0-alpha.24",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "fontsource-roboto": "^4.0.0",
    "jest-junit": "^12.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-number-format": "^4.6.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "serve": "^11.3.2",
    "styled-components": "^5.3.0",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "npx eslint src --ext .js,.jsx",
    "test": "react-scripts test --coverage --watchAll=false --testResultsProcessor ./node_modules/jest-sonar-reporter/index.js",
    "test:ci": "react-scripts test --coverage --watchAll=false --testResultsProcessor ./node_modules/jest-sonar-reporter/index.js",
    "test:dev": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "devDependencies": {
    "@testing-library/user-event": "^12.8.3",
    "eslint": "^7.29.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "jest-sonar-reporter": "^2.0.0"
  }
}
